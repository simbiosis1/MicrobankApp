<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:org.simbiosis.ui.bprs.loan.client.editor"
	xmlns:gr="urn:import:org.kembang.grid.client">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel>
			<table class="formTableLayout">
				<tr>
					<td class="formLabel">
						No rek
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="code" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Tanggal registrasi
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="strRegistration" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Produk
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="strProduct" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Tanggal akad
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="strContractDate" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						No akad
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="contract" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						AO
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="strAo" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">AO sebelumnya</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="aoHistory" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Tujuan pembiayaan
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="purpose" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Sektor ekonomi
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="biSektor" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Pokok
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="strPrincipal" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Rate
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:HorizontalPanel>
							<g:Label ui:field="strRate" styleName="formTextlabel" />
							<g:HTML styleName="formTextlabel">&nbsp;&nbsp;% pa</g:HTML>
						</g:HorizontalPanel>
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Biaya administrasi
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="strAdmin" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Biaya denda (harian)
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="strFine" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td colspan="3">&nbsp;</td>
				</tr>
				<tr>
					<td class="formLabel">
						Jangka waktu
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:HorizontalPanel>
							<g:Label ui:field="strTenor" styleName="formTextlabel" />
							<g:HTML styleName="formTextlabel">&nbsp;&nbsp;bulan</g:HTML>
						</g:HorizontalPanel>
					</td>
				</tr>
				<tr>
					<td colspan="3">&nbsp;</td>
				</tr>
				<tr>
					<td colspan="3" class="formLabel">Data angsuran</td>
				</tr>
				<tr>
					<td colspan="3">
						<g:VerticalPanel width="420px">
							<e:LoanScheduleViewerTable
								ui:field="schedules" width="100%" height="300px" />
							<g:FlexTable ui:field='schedFooter' styleName='Global-FlexTable-Footer'
								cellSpacing='0' cellPadding='0' />
						</g:VerticalPanel>
					</td>
				</tr>
				<tr>
					<td colspan="3">&nbsp;</td>
				</tr>
			</table>
