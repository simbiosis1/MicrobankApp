<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:e="urn:import:org.simbiosis.ui.bprs.teller.client.editor"
	xmlns:k="urn:import:org.kembang.editor.client"
	>
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel>
			<table>
				<tr>
					<td colspan="3" class="formDescription">Transaksi kas</td>
				</tr>
				<tr>
					<td class="formLabel">Tanggal</td>
					<td>&nbsp;</td>
					<td>
						<g:Label ui:field="strDate" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Kode</td>
					<td>&nbsp;</td>
					<td>
						<g:Label ui:field="code" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Referensi</td>
					<td>&nbsp;</td>
					<td>
						<g:TextBox styleName="formText" ui:field="refCode" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Jenis</td>
					<td>&nbsp;</td>
					<td>
						<e:CashTypeEditor ui:field="type" styleName="formCombobox" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Penerima / Penyetor</td>
					<td>&nbsp;</td>
					<td>
						<g:TextBox ui:field="maker" styleName="formText" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Deskripsi</td>
					<td>&nbsp;</td>
					<td>
						<g:TextBox styleName="formText" ui:field="description"
							width="200px" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Jumlah</td>
					<td>&nbsp;</td>
					<td>
						<k:DoubleTextBox styleName="formText" ui:field="value" />
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 