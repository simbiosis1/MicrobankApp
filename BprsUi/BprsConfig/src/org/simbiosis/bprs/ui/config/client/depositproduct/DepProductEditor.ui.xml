<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:org.simbiosis.bprs.ui.config.client.editor"
	xmlns:k="urn:import:org.kembang.editor.client">
	<g:HTMLPanel>
		<table>
			<tr>
				<td class="formLabel">Kode</td>
				<td>
					<g:TextBox ui:field="code" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Nama</td>
				<td>
					<g:TextBox ui:field="name" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Nisbah</td>
				<td>
					<k:DoubleTextBox ui:field="sharing" styleName="formText" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Tempo</td>
				<td>
					<e:TermListBox ui:field="term" styleName="formCombobox" />
				</td>
			</tr>
			<tr>
				<td class="formSubtitle" colspan="2">Akun</td>
			</tr>
			<tr>
				<td class="formLabel">Produk</td>
				<td>
					<e:CoaListEditor ui:field="coa1" styleName="formCombobox" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Bagi Hasil</td>
				<td>
					<e:CoaListEditor ui:field="coa2" styleName="formCombobox" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Cadangan Bagi Hasil</td>
				<td>
					<e:CoaListEditor ui:field="coa3" styleName="formCombobox" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 