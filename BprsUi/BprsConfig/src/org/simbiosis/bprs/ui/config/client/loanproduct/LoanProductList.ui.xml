<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:e="urn:import:org.simbiosis.bprs.ui.config.client.editor"
	>
	<ui:style>
		.leftside {
			padding-left: 10px;
			height: 100%
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX">
		<g:west size="400">
			<e:ProductTable ui:field="users"/>
		</g:west>
		
		<g:center>
			<g:HorizontalPanel ui:field="userForm" styleName="{style.leftside}" width="500px" />
		</g:center>
	</g:DockLayoutPanel>
	
</ui:UiBinder> 