<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel>
			<table class="formTableLayout">
				<tr>
					<td colspan="3" class="formDescription">Transaksi kas</td>
				</tr>
				<tr>
					<td class="formLabel">Tanggal</td>
					<td>&nbsp;</td>
					<td>
						<g:DateLabel ui:field="date" styleName="formTextlabel" customFormat="dd-MM-yyyy"/>
					</td>
				</tr>
				<tr>
					<td class="formLabel">Kode</td>
					<td>&nbsp;</td>
					<td>
						<g:Label ui:field="code" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Referensi</td>
					<td>&nbsp;</td>
					<td>
						<g:Label ui:field="refCode" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Jenis</td>
					<td>&nbsp;</td>
					<td>
						<g:Label ui:field="strType" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Penerima / Penyetor</td>
					<td>&nbsp;</td>
					<td>
						<g:Label ui:field="maker" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Deskripsi</td>
					<td>&nbsp;</td>
					<td>
						<g:Label ui:field="description" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">Jumlah</td>
					<td>&nbsp;</td>
					<td>
						<g:NumberLabel ui:field="value" styleName="formTextlabel" customFormat="#,##0.00"/>
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 