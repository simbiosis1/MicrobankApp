<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<table>
			<tr>
				<td colspan="3">Transaksi yang anda lakukan memerlukan otorisasi.
					Tekan tombol "Konfirmasi" untuk mendapatkan otorisasi.</td>
			</tr>
			<tr>
				<td width="100%">
					<g:Label ui:field="labelStatus" styleName="formTextlabel">Belum otorisasi</g:Label>
				</td>
				<td>
					<g:Button ui:field="btnConfirm" styleName="formButton">Konfirmasi</g:Button>
				</td>
				<td>
					<g:Button ui:field="btnExecute" styleName="formButton">Lanjutkan</g:Button>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 