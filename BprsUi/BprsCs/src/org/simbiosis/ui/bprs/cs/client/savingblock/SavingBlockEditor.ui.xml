<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:org.simbiosis.ui.bprs.common.client.savinghelper"
	xmlns:e="urn:import:org.simbiosis.ui.bprs.cs.client.editor">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel>
			<table class="formTableLayout" width="100%">
				<tr>
					<td class="formDescription">Blokir tabungan</td>
				</tr>
				<tr>
					<td class="formSubtitle">Data tabungan</td>
				</tr>
				<tr>
					<td width="100%">
						<s:SavingInfo ui:field="saving" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">&nbsp;</td>
				</tr>
				<tr>
					<td class="formSubtitle">Data blokir</td>
				</tr>
				<tr>
					<td class="formSubtitle">
						<g:VerticalPanel width="650px">
							<e:SavingBlockirTable width="100%" height="150px"
								ui:field="blockir" />
							<g:HTMLPanel>&nbsp;</g:HTMLPanel>
							<g:HorizontalPanel>
								<g:Button styleName="formButton" ui:field="add">+</g:Button>
								<g:HTMLPanel>&nbsp;&nbsp;</g:HTMLPanel>
								<g:Button styleName="formButton" ui:field="remove">-</g:Button>
							</g:HorizontalPanel>
						</g:VerticalPanel>

					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 