<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:e="urn:import:org.simbiosis.ui.bprs.teller.client.editor"
	xmlns:p3="urn:import:org.simbiosis.ui.bprs.teller.client.editor">
	<g:VerticalPanel>
		<e:SavingUploadCollectiveTable ui:field="ctTable"
			width="950px" height="400px" />
		<g:FlexTable ui:field='transactionFooter' styleName='Global-FlexTable-Footer'
						cellSpacing='0' cellPadding='0' />
		<g:HTMLPanel>
			<table>
				<tr>
					<td class="formLabel">Teller / Marketing</td>
					<td>&nbsp;</td>
					<td>
						<p3:TellerListEditor ui:field="tellers" styleName="formCombobox" />
					</td>
				</tr>
			</table>
			<br />
		</g:HTMLPanel>
		<g:Button styleName="formButton" ui:field="btnExecute">Jalankan transaksi</g:Button>
	</g:VerticalPanel>
</ui:UiBinder> 