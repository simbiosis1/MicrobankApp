<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:e="urn:import:org.simbiosis.ui.bprs.loan.client.editor" xmlns:gr="urn:import:org.kembang.grid.client">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel>
			<table class="formTableLayout">
				<tr>
					<td class="formLabel">
						No jaminan
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="code" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Tanggal registrasi
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:DateLabel ui:field="registration" styleName="formTextlabel"
							customFormat="dd-MM-yyyy" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Jenis
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="strType" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Jenis dokumen/No rekening
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="number" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Deskripsi
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="description" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Pemilik
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:Label ui:field="ownerName" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Nilai taksiran internal
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:NumberLabel ui:field="appraisalIntValue"
							styleName="formTextlabel" customFormat="#,##0.00" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Nilai taksiran pasar
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:NumberLabel ui:field="appraisalMarkValue"
							styleName="formTextlabel" customFormat="#,##0.00" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Nilai sesuai OJK
					</td>
					<td>&nbsp;&nbsp;</td>
					<td>
						<g:NumberLabel ui:field="appraisalOJKValue"
							styleName="formTextlabel" customFormat="#,##0.00" />
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 