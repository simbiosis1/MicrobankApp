<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel>
			<table class="formTableLayout">
				<tr>
					<td class="formLabel">
						No rek
					</td>
					<td>
						<g:Label ui:field="code" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Tanggal registrasi
					</td>
					<td>
						<g:DateLabel ui:field="registration" styleName="formTextlabel" customFormat="dd-MM-yyyy"/>
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Bilyet
					</td>
					<td>
						<g:Label ui:field="bilyet" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Produk
					</td>
					<td>
						<g:Label ui:field="strProduct" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Nominal
					</td>
					<td>
						<g:NumberLabel ui:field="value" styleName="formTextlabel" customFormat="#,##0.00"/>
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Nisbah
					</td>
					<td>
						<g:NumberLabel ui:field="rate" styleName="formTextlabel"  customFormat="#,##0.00"/>
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Tambahan nisbah
					</td>
					<td>
						<g:NumberLabel ui:field="specialRate" styleName="formTextlabel"  customFormat="#,##0.00"/>
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						ARO
					</td>
					<td>
						<g:Label ui:field="strAro" styleName="formTextlabel" />
					</td>
				</tr>
				<tr>
					<td class="formLabel">
						Zakat
					</td>
					<td>
						<g:Label ui:field="strZakat" styleName="formTextlabel" />
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 