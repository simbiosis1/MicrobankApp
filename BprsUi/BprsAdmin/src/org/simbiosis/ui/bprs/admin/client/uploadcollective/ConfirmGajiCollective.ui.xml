				<tr>
					<td class="formLabel"><g:RadioButton ui:field="typeAcc" name="sumberDana">Rekening tabungan</g:RadioButton></td>
					<td>&nbsp;</td>
					<td>
					</td>
				</tr>
			</table>
			<br />
		</g:HTMLPanel>
		<g:Button styleName="formButton" ui:field="btnExecute">Jalankan pindah
			buku</g:Button>
	</g:VerticalPanel>
</ui:UiBinder> 